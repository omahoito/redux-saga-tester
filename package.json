{
  "name": "@oda-omaolo/redux-saga-tester",
  "version": "1.0.1",
  "description": "Full redux environment testing helper for redux-saga",
  "main": "dist/SagaTester.js",
  "scripts": {
    "lint": "eslint './src/**/*.js' './test/**/*.js' './examples/**/*.js'",
    "examples": "mocha --require babel-register './examples/**/*.js'",
    "prepublish": "npm test && npm run compile",
    "test": "mocha --require babel-register && npm run lint",
    "test-dev": "mocha --watch --require babel-register",
    "compile": "babel -d dist/ src/",
    "build": ":"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omahoito/redux-saga-tester.git"
  },
  "keywords": [
    "redux-saga",
    "test"
  ],
  "author": "Guy Benron",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.19.0",
    "immutable": "^3.8.2",
    "mocha": "^6.0.2",
    "nock": "^9.0.13",
    "node-fetch": "^1.6.3",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "dependencies": {}
}
